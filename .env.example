# Model configuration
AGENT_MODEL=qwen2.5-coder-7b
AGENT_MODEL_URL=http://localhost:8000/v1
AGENT_MODEL_API_KEY=
AGENT_MODEL_PATH=

# Gateway: "local" (default) or "gemini"
AGENT_GATEWAY=local

# Gemini API (only used when AGENT_GATEWAY=gemini)
GEMINI_API_KEY=
GEMINI_MODEL=gemini-2.5-flash

# Agent configuration
AGENT_MAX_STEPS=20
AGENT_TEMPERATURE=0.7
AGENT_MAX_TOKENS=4096

# Tool configuration
AGENT_ENABLE_SHELL=true
AGENT_ENABLE_FILES=true
AGENT_ENABLE_FETCH=true
AGENT_ENABLE_DATA_VIEW=true
AGENT_ENABLE_PYEXE=true
AGENT_ENABLE_MEMORY=true
AGENT_ENABLE_CHUNK_SEARCH=true
AGENT_ENABLE_PATCH=true

# Storage configuration
AGENT_STORE_TYPE=memory
AGENT_STORE_PATH=./data

# Server configuration
AGENT_HOST=0.0.0.0
AGENT_PORT=8000
AGENT_ENABLE_HTTP=false

# Logging
AGENT_LOG_LEVEL=INFO
